{
  "name": "e2e-test-framework",
  "private": true,
  "version": "1.0.0",
  "description": "E2E UI Test Framework",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "postinstall": "",
    "doc": "node_modules/.bin/jsdoc -c jsdoc-conf.json",
    "selenium-update": "./node_modules/.bin/webdriver-manager update --ignore_ssl --gecko=false",
    "selenium-start": "./node_modules/.bin/webdriver-manager start",
    "e2e": "./node_modules/.bin/protractor conf.js",
    "e2e-headless": "./node_modules/.bin/protractor conf.js --params.headless=true",
    "e2e-url": "./node_modules/.bin/protractor conf.js --params.url=https://localhost:5000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teamco/e2e"
  },
  "keywords": [
    "AIA",
    "E2E",
    "UI",
    "Test"
  ],
  "author": "Vladimir Tkach",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "colors": "^1.3.3",
    "eslint-plugin-protractor": "^1.41.1",
    "protractor": "^5.4.2",
    "ps-list": "^6.3.0",
    "selenium-webdriver": "^3.6.0",
    "webdriver-manager": "^13.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "boom": "^7.3.0",
    "chromedriver": "^2.46.0",
    "cryptiles": "^4.1.3",
    "docdash": "^1.1.0",
    "eslint": "^5.16.0",
    "hoek": "^6.1.3",
    "html-dnd": "^1.2.0",
    "jasmine": "^3.4.0",
    "jasmine2-custom-message": "^0.9.3",
    "jsdoc": "^3.5.5",
    "moment": "^2.24.0",
    "protractor-pretty-html-reporter": "0.0.16"
  }
}
